[access]
	inheritFrom = All-Projects
[submit]
[access "refs/*"]
	owner = group user/nowakm7
[access "refs/for/refs/*"]
	label-Code-Review = -2..+2 group user/nowakm7
	label-Verified = -1..+1 group user/nowakm7
	push = group user/nowakm7
	read = group user/nowakm7
	rebase = group user/nowakm7
	removeReviewer = group user/nowakm7
	submit = group user/nowakm7
[access "refs/heads/*"]
	create = group user/nowakm7
	push = +force group user/nowakm7
	pushMerge = group user/nowakm7
	read = group user/nowakm7
[access "refs/tags/*"]
	push = group user/nowakm7
	push = +force group user/nowakm7
